{
  "name": "library-management-system",
  "version": "1.0.0",
  "description": "Library Management System",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd BackEnd && dotnet run",
    "frontend": "cd FrontEnd && npm run dev",
    "install-all": "cd FrontEnd && npm install --legacy-peer-deps && cd ../BackEnd && dotnet restore",
    "build": "cd FrontEnd && npm run build",
    "vercel-install": "cd FrontEnd && npm install --legacy-peer-deps",
    "postinstall": "cd FrontEnd && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}